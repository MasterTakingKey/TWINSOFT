<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="489.0" prefWidth="554.0" stylesheets="@../application/estilo1.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ControladorModosDeJuego">
   <children>
      <ImageView fitHeight="558.0" fitWidth="792.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../imagenes/menu_principal.png" />
         </image>
      </ImageView>
      <Button fx:id="partidaEstandar" layoutX="300.0" layoutY="191.0" mnemonicParsing="false" onAction="#partidaEstandarHandler" prefHeight="54.0" prefWidth="193.0" text="ESTANDAR">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button fx:id="modoLibre" layoutX="300.0" layoutY="260.0" mnemonicParsing="false" onAction="#modoLibreHandler" prefHeight="54.0" prefWidth="193.0" text="LIBRE">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="partPorCarta" layoutX="300.0" layoutY="334.0" mnemonicParsing="false" onAction="#porCartaHandler" prefHeight="54.0" prefWidth="193.0" text="POR CARTA">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <StackPane layoutX="350.0" layoutY="427.0" prefHeight="88.0" prefWidth="93.0" styleClass="fondoBoton" stylesheets="@../application/estilo1.css">
         <children>
            <ImageView fx:id="iconoSonido" fitHeight="54.0" fitWidth="58.0" onMouseClicked="#clickSound" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../imagenes/sonido_on.png" />
               </image>
            </ImageView>
         </children>
      </StackPane>
   </children>
</AnchorPane>
